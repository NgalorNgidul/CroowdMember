<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:org.simbiosis.ui.gwt.client.editor"
	xmlns:e="urn:import:com.croowd.ui.validation.client.userform"
	>
	<g:ScrollPanel>
		<g:VerticalPanel styleName="apppanel" ui:field="appPanel">
			<g:VerticalPanel styleName="fullbox">
				<g:HTMLPanel styleName="header1">
					<div>DATA DIRI</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Nama</div>
										<g:TextBox styleName="fieldtextbox" ui:field="name" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Jenis kelamin</div>
										<e:IntegerTypeComboBox styleName="fieldtextbox"
											ui:field="sex" />
									</div>
									<div class="compositetablebox">
										<div class="compositecellbox">
											<div class="fieldtitle">Jenis identitas</div>
											<e:IntegerTypeComboBox styleName="fieldcombobox"
												ui:field="idType" />
										</div>
										<div class="compositecellbox">
											<div class="fieldtitle">Nomor identitas</div>
											<s:NumericTextBox styleName="fieldtextbox"
												ui:field="idCode" maxLength="16" visibleLength="16" />
										</div>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Email</div>
										<g:Label styleName="fieldlabel" ui:field="email" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Alamat</div>
										<g:TextBox styleName="fieldtextbox" ui:field="address" />
									</div>
									<div class="compositetablebox">
										<div class="compositecellbox">
											<div class="fieldtitle">Kota</div>
											<g:TextBox styleName="fieldtextbox" ui:field="city" />
										</div>
										<div class="compositecellbox">
											<div class="fieldtitle">Kode pos</div>
											<s:NumericTextBox styleName="fieldtextbox"
												ui:field="zipCode" maxLength="5" visibleLength="5" />
										</div>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Propinsi</div>
										<g:TextBox styleName="fieldtextbox" ui:field="province" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Nomer telpon</div>
										<div>
											<g:TextBox styleName="fieldtextbox" ui:field="fixPhone" />
											<g:TextBox styleName="fieldtextbox" ui:field="cellPhone" />
										</div>
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="header1">
					<div>KEAMANAN</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Password</div>
										<g:PasswordTextBox styleName="fieldtextbox"
											ui:field="password" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Konfirmasi</div>
										<g:PasswordTextBox styleName="fieldtextbox"
											ui:field="passwordconf" />
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel>
					<g:Button styleName="fieldbigbutton" ui:field="btnSave">Simpan</g:Button>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:ScrollPanel>
</ui:UiBinder> 