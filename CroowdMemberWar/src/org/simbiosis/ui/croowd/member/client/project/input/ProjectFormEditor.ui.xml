<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
	
	</ui:style>
	<g:AbsolutePanel styleName="background">
		<g:ScrollPanel>
			<g:VerticalPanel styleName="fullbox">
				<g:HTMLPanel styleName="header1">
					<div>DATA PROJECT</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox" height="100%">
					<g:HTMLPanel styleName="fullbox" height="100%">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Project Title</div>
										<g:TextBox styleName="fieldtextbox" ui:field="title"
											width="300px"></g:TextBox>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Short Blurb</div>
										<g:TextArea styleName="fieldtextarea" ui:field="shortBlurb"
											height="50px" width="300px">
										</g:TextArea>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Category</div>
										<g:VerticalPanel>
											<g:ListBox styleName="fieldcombobox" ui:field="category"
												width="300px"></g:ListBox>
											<g:ListBox styleName="fieldcombobox" ui:field="subCategory"
												width="300px"></g:ListBox>
										</g:VerticalPanel>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Project Location</div>
										<g:ListBox styleName="fieldcombobox" ui:field="location"
											width="300px"></g:ListBox>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Funding Duration </div>
										<d:DateBox styleName="fielddatepicker" ui:field="duration"
											width="300px"></d:DateBox>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Funding Goal</div>
										<g:TextBox styleName="fieldtextbox" ui:field="goal"
											width="300px"></g:TextBox>
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel spacing="5">
					<g:Button styleName="fieldbigbutton" ui:field="btnSave">Save</g:Button>
					<g:Button styleName="fieldbigbutton" ui:field="btnCancel">Cancel</g:Button>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:ScrollPanel>
	</g:AbsolutePanel>
</ui:UiBinder> 