<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g1="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:ScrollPanel styleName="scrollpanel" height="100%"
		width="100%">
		<g:VerticalPanel styleName="apppanel" ui:field="appPanel"
			height="100%" width="100%">
			<g:VerticalPanel styleName="fullbox">
				<g:HTMLPanel styleName="header1">
					<div>DATA DIRI</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Nama</div>
										<g:TextBox styleName="fieldtextbox" ui:field="name" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Tempat dan tanggal lahir</div>
										<g:TextBox styleName="fieldtextbox" ui:field="pob" />
										<g1:DateBox styleName="fieldtextbox" ui:field="dob" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Tanda pengenal</div>
										<g:TextBox styleName="fieldtextbox" ui:field="idtype" />
										<g:TextBox styleName="fieldtextbox" ui:field="idcode" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Nama ibu kandung</div>
										<g:TextBox styleName="fieldtextbox" ui:field="motherName" />
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="header1">
					<div>ALAMAT</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Email</div>
										<g:Label styleName="fieldlabel" ui:field="email" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Jalan</div>
										<g:TextBox styleName="fieldtextbox" ui:field="address" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Kota, kode pos</div>
										<div>
											<g:TextBox styleName="fieldtextbox" ui:field="city" />
											<g:TextBox styleName="fieldtextbox" ui:field="zipCode" />
										</div>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Propinsi</div>
										<g:TextBox styleName="fieldtextbox" ui:field="province" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Nomer telpon</div>
										<g:TextBox styleName="fieldtextbox" ui:field="phone" />
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel>
					<g:Button styleName="fieldbigbutton" ui:field="btnSave">Simpan</g:Button>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:ScrollPanel>

</ui:UiBinder> 