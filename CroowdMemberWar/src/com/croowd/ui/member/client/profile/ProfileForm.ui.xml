<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g1="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:org.simbiosis.ui.gwt.client.editor" xmlns:e="urn:import:com.croowd.ui.member.client.component">
	<g:ScrollPanel styleName="scrollpanel" height="100%"
		width="100%">
		<g:VerticalPanel styleName="apppanel" ui:field="appPanel"
			height="100%" width="100%">
			<g:VerticalPanel styleName="fullbox">
				<g:HTMLPanel styleName="header1">
					<div>DATA DIRI</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Nama</div>
										<g:TextBox styleName="fieldtextbox" ui:field="name" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Jenis kelamin</div>
										<e:IntegerTypeComboBox styleName="fieldtextbox"
											ui:field="sex" />
									</div>
									<div class="compositetablebox">
										<div class="compositecellbox">
											<div class="fieldtitle">Tempat lahir</div>
											<g:TextBox styleName="fieldtextbox" ui:field="pob" />
										</div>
										<div class="compositecellbox">
											<div class="fieldtitle">Tanggal lahir</div>
											<e:DateSelectorBox ui:field="dob" />
										</div>
									</div>
									<div class="compositetablebox">
										<div class="compositecellbox">
											<div class="fieldtitle">Jenis identitas</div>
											<e:IntegerTypeComboBox styleName="fieldcombobox"
												ui:field="idType" />
										</div>
										<div class="compositecellbox">
											<div class="fieldtitle">Nomor identitas</div>
											<s:IntegerTextBox styleName="fieldtextbox"
												ui:field="idCode" maxLength="16" visibleLength="16" />
										</div>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Nama ibu kandung</div>
										<g:TextBox styleName="fieldtextbox" ui:field="motherName" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Pendidikan</div>
										<e:IntegerTypeComboBox styleName="fieldcombobox"
											ui:field="education" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">NPWP</div>
										<s:IntegerTextBox styleName="fieldtextbox"
											ui:field="taxNr" maxLength="15" visibleLength="15" />
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="header1">
					<div>ALAMAT</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Email</div>
										<g:Label styleName="fieldlabel" ui:field="email" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Alamat</div>
										<g:TextBox styleName="fieldtextbox" ui:field="address" />
									</div>
									<div class="compositetablebox">
										<div class="compositecellbox">
											<div class="fieldtitle">Kota</div>
											<g:TextBox styleName="fieldtextbox" ui:field="city" />
										</div>
										<div class="compositecellbox">
											<div class="fieldtitle">Kode pos</div>
											<s:IntegerTextBox styleName="fieldtextbox"
												ui:field="zipCode" maxLength="5" visibleLength="5" />
										</div>
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Propinsi</div>
										<g:TextBox styleName="fieldtextbox" ui:field="province" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Nomer telpon</div>
										<div>
											<g:TextBox styleName="fieldtextbox" ui:field="fixPhone" />
											<g:TextBox styleName="fieldtextbox" ui:field="cellPhone" />
										</div>
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="header1">
					<div>PEKERJAAN UTAMA</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Sumber penghasilan</div>
										<e:IntegerTypeComboBox styleName="fieldcombobox"
											ui:field="mainIncomeType" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Jumlah penghasilan</div>
										<s:DoubleTextBox styleName="fieldtextbox"
											ui:field="mainIncome" />
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="header1">
					<div>UNTUK PENCARI DANA</div>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="headerspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel styleName="widgetbox">
					<g:HTMLPanel styleName="fullbox">
						<table class="fullbox">
							<tr>
								<td class="halfbox">
									<div class="compositebox">
										<div class="fieldtitle">Sumber penghasilan sampingan</div>
										<e:IntegerTypeComboBox styleName="fieldcombobox"
											ui:field="sideIncomeType" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Jumlah penghasilan sampingan</div>
										<s:DoubleTextBox styleName="fieldtextbox"
											ui:field="sideIncome" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Jumlah pengeluaran per bulan</div>
										<s:DoubleTextBox styleName="fieldtextbox"
											ui:field="expense" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Jumlah pengeluaran untuk pinjaman lain</div>
										<s:DoubleTextBox styleName="fieldtextbox"
											ui:field="otherExpense" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Status tempat tinggal, lama tinggal</div>
										<e:IntegerTypeComboBox styleName="fieldcombobox"
											ui:field="home" />
										<s:IntegerTextBox styleName="fieldtextbox"
											ui:field="homeStayDur" width="50px" />
									</div>
									<div class="compositebox">
										<div class="fieldtitle">Kepemilikan kendaraan, tahun pembuatan</div>
										<e:IntegerTypeComboBox styleName="fieldcombobox"
											ui:field="vehicle" />
										<s:IntegerTextBox styleName="fieldtextbox"
											ui:field="vehicleProduction" width="50px" />
									</div>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:HorizontalPanel>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div></div>
				</g:HTMLPanel>
				<g:HorizontalPanel>
					<g:Button styleName="fieldbigbutton" ui:field="btnSave">Simpan</g:Button>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:ScrollPanel>

</ui:UiBinder> 