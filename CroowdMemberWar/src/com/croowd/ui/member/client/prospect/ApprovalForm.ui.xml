<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:VerticalPanel styleName="fullbox" height="100%"
		width="100%">
		<g:HTMLPanel styleName="header1">
			<div>DATA ANGGOTA</div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="headerspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HorizontalPanel styleName="widgetbox">
			<g:HTMLPanel styleName="fullbox">
				<table class="fullbox">
					<tr>
						<td class="halfbox">
							<div class="compositebox">
								<div class="fieldtitle">Nama</div>
								<g:Label styleName="fieldlabel" ui:field="ownerName" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Email</div>
								<g:Label styleName="fieldlabel" ui:field="email" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Alamat</div>
								<g:Label styleName="fieldlabel" ui:field="address" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Kota, kode pos</div>
								<div>
									<g:Label styleName="fieldlabel" ui:field="city" />
									,
									<g:Label styleName="fieldlabel" ui:field="zipCode" />
								</div>
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Propinsi</div>
								<g:Label styleName="fieldlabel" ui:field="province" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Nomer telpon</div>
								<g:Label styleName="fieldlabel" ui:field="phone" />
							</div>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:HorizontalPanel>
		<g:HTMLPanel styleName="widgetboxspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="header1">
			<div>DATA PROYEK</div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="headerspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HorizontalPanel styleName="widgetbox">
			<g:HTMLPanel styleName="fullbox">
				<table class="fullbox">
					<tr>
						<td class="halfbox">
							<div class="compositebox">
								<div class="fieldtitle">Nama proyek</div>
								<g:Label styleName="fieldlabel" ui:field="title" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Lokasi</div>
								<g:Label styleName="fieldlabel" ui:field="location" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Kebutuhan</div>
								<g:Label styleName="fieldlabel" ui:field="principal" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Tenor</div>
								<g:Label styleName="fieldlabel" ui:field="tenor" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Keterangan</div>
								<g:Label styleName="fieldlabel" ui:field="description" />
							</div>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:HorizontalPanel>
		<g:HTMLPanel styleName="widgetboxspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Button styleName="fieldbigbutton" ui:field="btnApprove">Setujui</g:Button>
			<g:HTMLPanel styleName="widgetboxspacer">
				<div class="fieldspacer"></div>
			</g:HTMLPanel>
			<g:Button styleName="fieldbigbutton" ui:field="btnReject">Tolak</g:Button>
			<g:cell width="100%" horizontalAlignment="ALIGN_RIGHT">
        		<g:Button styleName="fieldbigbutton" ui:field="btnBack" text="Kembali" />
    		</g:cell>
		</g:HorizontalPanel>
	</g:VerticalPanel>

</ui:UiBinder> 