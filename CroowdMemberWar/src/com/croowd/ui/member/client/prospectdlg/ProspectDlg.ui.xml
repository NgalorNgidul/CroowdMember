<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DialogBox autoHide="true" modal="true">
		<g:caption>
			<b>CARI PROSPEK</b>
		</g:caption>
		<g:VerticalPanel width="100%" height="100%">
			<g:HorizontalPanel width="100%">
				<g:TextBox width="100%" styleName="fieldtextbox" ui:field="searchKey" />
				<g:HTMLPanel styleName="widgetboxspacer">
					<div class="fieldspacer"></div>
				</g:HTMLPanel>
				<g:Button styleName="fieldbutton" ui:field="btnSearch">Cari</g:Button>
			</g:HorizontalPanel>
			<g:cell height="70%">
				<g:ScrollPanel width="100%" height="100%">
					<g:VerticalPanel ui:field="searchResult" width="100%"
						height="100%" />
				</g:ScrollPanel>
			</g:cell>
			<g:HTMLPanel styleName="widgetboxspacer">
				<div></div>
			</g:HTMLPanel>
			<g:HorizontalPanel>
				<g:Button styleName="fieldbigbutton" ui:field="btnCancel"
					text="Tutup" />
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:DialogBox>
</ui:UiBinder> 